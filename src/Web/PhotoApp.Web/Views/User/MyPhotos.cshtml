@model PhotoApp.Web.ViewModels.MyPhotosViewModel
@{
    ViewData["Title"] = "My photos";

}


<div class="container">
    <div class="card-columns">
        @foreach (var item in Model.Photos)
        {
            <div class="card">
                <img src="@item.PhotoLink" class="card-img-top" alt="...">
            </div>
        }
    </div>
</div>